{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "func-names": ["error", "never"],
    "semi": [2, "never"],
    "no-unused-expressions": "off",
  },
  "env":{
    "browser": true,
    "mocha":true,
    "node":true
  }
}